{
  "name": "<%= projectName %>",
  "private": true,
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "analyze": "react-static build --analyze",
    "serve": "serve dist -p 3000",
    "create-pages-json": "node src/createPagesJson",
    "paperboy:local": "paperboy -c paperboy/paperboy.local.json",
    "paperboy:docker": "paperboy -c paperboy/paperboy.docker.json",
    "s3-sync": "s3surgeon --access-key-id $ACCESS_KEY_ID --secret-access-key $SECRET_ACCESS_KEY --bucket $BUCKET --directory dist && aws cloudfront create-invalidation --distribution-id $DISTRIBUTION_ID --paths '/*' 2>&1 1>/dev/null"
  },
  "dependencies": {
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-static": "7.0.0",
    "react-static-plugin-reach-router": "7.0.0",
    "react-static-plugin-sitemap": "7.0.0",
    "react-static-plugin-source-filesystem": "7.0.0"
  },
  "devDependencies": {
<% if (restVersion === '1') { %>
    "@neoskop/magnolia-headless-tools": "0.0.6",
<% } %>
    "@neoskop/paperboy-cli": "<%= paperboyVersion %>",
    "@neoskop/s3surgeon": "0.1.2",
    "babel-eslint": "9.x",
    "eslint": "5.x",
    "eslint-config-react-app": "3.0.8",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "react-hot-loader": "4.11.1",
    "serve": "10.1.2"
  },
  "author": "Neoskop GmbH"
}